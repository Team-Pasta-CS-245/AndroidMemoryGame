<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lennyyang.memorygameproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Game2x2Activity"></activity>
        <activity android:name=".Game2x3Activity"></activity>
        <activity android:name=".Game2x4Activity"></activity>
        <activity android:name=".Game2x5Activity"></activity>
        <activity android:name=".Game3x4Activity"></activity>
        <activity android:name=".Game2x7Activity"></activity>
        <activity android:name=".Game4x4Activity"></activity>
        <activity android:name=".Game3x6Activity"></activity>
        <activity android:name=".Game4x5Activity"></activity>
        <activity android:name=".ScoreActivity"></activity>
    </application>


</manifest>